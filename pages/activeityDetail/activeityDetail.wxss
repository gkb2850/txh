.container .title_box .txtbt {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(50,57,67,1);
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.container .title_box .txt {
  display: flex;
  font-size: 26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(99,109,123,1);
  padding: 0 24rpx;
  box-sizing: border-box;
}

.container .title_box .txt>view:nth-child(1) {
  height: 40rpx;
  line-height: 40rpx;
  width: 100rpx;
}

.container .title_box .txt>view:nth-child(2) {
  flex: 1;
  line-height: 40rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.container .title_box .people {
  display: flex;
  font-size: 26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(99,109,123,1);
  padding: 0 24rpx;
  box-sizing: border-box;
}

.container .title_box .people>view:nth-child(1) {
  height: 60rpx;
  line-height: 60rpx;
  width: 100rpx;
}

.container .title_box .people>view:nth-child(2) {
  flex: 1;
  line-height: 60rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  height: 60rpx;
}

.container .cont {
  padding: 20rpx 0;
}

.container .time_box {
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.container .time_box .time {
  flex: 1;
  display: flex;
  font-size: 28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,159,168,1);
}

.container .time_box .time:nth-child(2) {
  justify-content: flex-end;
}

.container .time_box .time>view:nth-child(1) {
  height: 100%;
  line-height: 60rpx;
}

.container .time_box .time>view:nth-child(2) {
  height: 100%;
  line-height: 60rpx;
  margin-left: 20rpx;
}

.container .signupa_box {
  padding: 0 24rpx;
  box-sizing: border-box;
  height: 80rpx;
  margin: 20rpx 0;
}

.container .signupa_box>view {
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  border-radius: 10rpx;
  color: #fff;
  background: #2D82FF;
}

.container .signupa_box .del_act {
  background: #FA5051;
}